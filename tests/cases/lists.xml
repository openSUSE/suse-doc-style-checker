      <row>
       <entry>
        <para>
         Category
        </para>
       </entry>
       <entry>
        <para>
         List
        </para>
       </entry>
      </row>
     </thead>
     <tbody>
      <row>
       <entry>
        <para>
         This list is good:
        </para>
       </entry>
       <entry>
        <itemizedlist  xml:id="il.sdsc.valid.8">
